<h1>New Restaurant</h1>
<div id="form">
  <%= simple_form_for @restaurant do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :category %>
    <%= f.input :phone_number %>
    <%= f.submit class: "btn btn-secondary"%>
  <% end %>
</div>
<%= link_to "Back to Restaurants", restaurants_path, class: "back-to" %>
